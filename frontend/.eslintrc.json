{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "import/no-unresolved": "off",
      "react/jsx-filename-extension": "off",
      "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/config-overrides.js"]}],
      "consistent-return": "off"
    }
}